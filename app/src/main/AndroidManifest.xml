            android:name="com.example.trishudey.hubsystemhelper.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.trishudey.hubsystemhelper.Displat_the_LDAP"
            android:label="@string/title_activity_displat_the__ldap" >
        </activity>
        <activity
            android:name="com.example.trishudey.hubsystemhelper.NewActivity"
            android:label="@string/title_activity_displat_the__ldap" >
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
